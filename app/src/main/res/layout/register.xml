            android:layout_height="wrap_content"
            style="@style/TextView7"/>

        <TextView
            android:id="@+id/textViewreg8"
            style="@style/TextView8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/editText6"
            android:layout_marginTop="@dimen/ssize_42"
            android:layout_marginBottom="@dimen/size_10"/>
    </RelativeLayout>

    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        style="@style/ImageView9"/>


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        >

    </LinearLayout>


    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        style="@style/TextView10"
        android:textColor="#ffff"
        android:layout_marginLeft="@dimen/size_15"
        android:layout_alignBottom="@id/imageView10"
        android:layout_marginBottom="@dimen/size_40"
        android:layout_marginStart="@dimen/size_15" />


    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        style="@style/TextView9"/>



    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        style="@style/TextView11"
        android:layout_toEndOf="@id/textView9"
        android:layout_toRightOf="@id/textView9"
        android:layout_alignBottom="@id/imageView10"
        android:layout_marginBottom="@dimen/size_40"
